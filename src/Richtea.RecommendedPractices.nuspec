<?xml version="1.0" encoding="utf-8"?>
<package xmlns="http://schemas.microsoft.com/packaging/2013/05/nuspec.xsd">
    <metadata minClientVersion="4.0">
        <id>Richtea.RecommendedPractices</id>
        <version>$version$</version>
        <authors>Rich Tebb</authors>
        <license type="expression">ISC</license>
        <icon>images/PackageIcon.png</icon>
        <tags>recommended-practices</tags>
        <developmentDependency>true</developmentDependency>
        <requireLicenseAcceptance>false</requireLicenseAcceptance>
        <projectUrl>https://github.com/richtea/Richtea.RecommendedPractices</projectUrl>
        <description>Applies richtea standard / recommended engineering practices to a project.</description>
        <copyright>Copyright Â©2024 Rich Tebb</copyright>
        <readme>docs/README.md</readme>
        <repository type="git" url="https://github.com/richtea/Richtea.RecommendedPractices.git" branch="$branch$" commit="$commit$" />
        <dependencies>
            <dependency id="Microsoft.CodeAnalysis.NetAnalyzers" version="8.0.0" />
            <dependency id="SecurityCodeScan.VS2019" version="5.6.7" />
            <dependency id="StyleCop.Analyzers" version="1.2.0-beta.556" />
            <dependency id="SonarAnalyzer.CSharp" version="9.19.0.84025" />
        </dependencies>
    </metadata>
    <files>
        <file src="images\**" target="images" />
        <file src="common\**" target="common" />
        <file src="build\**" target="build" />
        <file src="build\**" target="buildMultiTargeting" />
        <file src="..\README.md" target="docs" />
    </files>
</package>